extends layouts/main
include mixins/videoBlock

block content
    .search__header 
        h3 Searching for #{searchingBy}
    .search__videos
        if videoList.length === 0
            h5 No Videos Found
        each item in videoList
            +videoBlock({
                videoFile: item.videoFile,
                title: item.title,
                views: item.views,
                id: item.id
            })